<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<fx:root xmlns="http://javafx.com/javafx/8"
         xmlns:fx="http://javafx.com/fxml/1"
         type="javafx.scene.layout.VBox"
         fx:controller="com.oaktwister.views.layouts.AccountsPane"
         alignment="CENTER"
         prefHeight="400.0" prefWidth="600.0"
         stylesheets="/styles/layouts/accounts-pane.css">
    <Label fx:id="titleLabel" text="Accounts"/>
    <ScrollPane fx:id="scrollPane" VBox.vgrow="ALWAYS" style="-fx-background-color: red;">
        <VBox fx:id="vbox"/>
    </ScrollPane>
    <HBox alignment="CENTER_RIGHT">
        <Button fx:id="addButton" text="Add" />
    </HBox>
</fx:root>
