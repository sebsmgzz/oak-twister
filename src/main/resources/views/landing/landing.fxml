<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.VBox?>

<VBox xmlns="http://javafx.com/javafx"
      xmlns:fx="http://javafx.com/fxml"
      fx:controller="com.oaktwister.views.landing.LandingViewController"
      prefHeight="400.0" prefWidth="600.0"
      styleClass="background">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
    <Label fx:id="label" styleClass="label" />
    <Button onAction="#onGreetButton" styleClass="button" text="Greet" />
    <Button onAction="#onGoToMainButton" styleClass="button" text="Go to main" />
</VBox>
